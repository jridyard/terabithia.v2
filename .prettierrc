{
    "singleQuote": true,
    "printWidth": 120,
    "editor.formatOnSave": true,
    "proseWrap": "always",
    "tabWidth": 4,
    "requireConfig": false,
    "useTabs": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": true,
    "prettier.bracketSameLine": true,
    "prettier.disableLanguages": ["html"],
    "html.format.enable": false
}
